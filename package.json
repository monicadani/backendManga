{
  "name": "red-social",
  "version": "1.0.0",
  "description": "Proyecto red social Manga",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t red-social .",
    "docker:run": "docker run -p 3000:3000 -d red-social",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.2.1",
    "@loopback/boot": "^2.4.1",
    "@loopback/core": "^2.9.3",
    "@loopback/openapi-v3": "^3.4.7",
    "@loopback/repository": "^2.11.0",
    "@loopback/rest": "^5.2.1",
    "@loopback/rest-explorer": "^2.2.8",
    "@loopback/service-proxy": "^2.3.6",
    "@sendgrid/mail": "^7.2.3",
    "@types/multer": "^1.4.3",
    "@types/passport-http": "^0.3.8",
    "core-util-is": "^1.0.2",
    "cross-env": "^7.0.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "generate-password": "^1.5.1",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-mongodb": "^5.3.0",
    "loopback-connector-twilio": "^2.0.0",
    "multer": "^1.4.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/eslint-config": "^8.0.4",
    "@loopback/testlab": "^3.2.2",
    "@types/node": "^10.17.28",
    "@types/passport-http-bearer": "^1.0.35",
    "eslint": "^7.6.0",
    "source-map-support": "^0.5.19",
    "typescript": "^3.9.7"
  }
}
